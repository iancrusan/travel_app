<h1>Welcome to Travel App!</h1>
<p>Checkout all the places I have been to!</p>

<%= @homeland %><br>

<% @countries.each do |country| %>
<p><%= country %></p>
<% end %>



<% @travel_pics.each do |pic| %>
<p><%= image_tag pic, class: "img-responsive" %></p>
<% end %>


<table class="table">
  <tr>
    <% @united_states.each do |key, value| %>
    <th><%= key.titleize %></th>
    <% end %>
  </tr>
  <tr>
    <% @united_states.each do |key, value| %>
    <% if value.kind_of?(Array) %>
    <td><%= value.join(", ") %></td>
    <% else %>
    <td><%= value %></td>
    <% end %>
    <% end %>
  </tr>
</table>
